<labConfiguration title="Radioactivity 2" version="4.0">
    <navmenuPhoto>
        <image>http://localhost:8080/RadioactivityLabServer/resources/img/radioactivity2.png</image>
    </navmenuPhoto>
    <labCamera>
        <url />
