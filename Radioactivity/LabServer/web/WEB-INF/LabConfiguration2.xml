    <configuration>
        <setup id="RadioactivityVsTime">
            <name>Radioactivity over Time</name>
            <description>
                <p>Measure radioactivity over time at a fixed distance from the source.</p>
            </description>
            <distance>20</distance>
            <duration>5</duration>
            <repeat>3</repeat>
        </setup>
        <setup id="RadioactivityVsDistance">
            <name>Radioactivity versus Distance</name>
            <description>
                <p>Measure radioactivity at various distances from the source.</p>
            </description>
            <distance>15,20,25,35,60</distance>
            <duration>5</duration>
            <repeat>3</repeat>
        </setup>
        <setup id="RadioactivityVsAbsorber">
            <name>Radioactivity versus Absorber</name>
            <description>
                <p>Measure radioactivity through various absorbers at a fixed distance from the source.</p>
            </description>
            <absorberName>None,Paper,Aluminium Sheet,Lead Sheet,Aluminium Disc,Lead Disc</absorberName>
            <distance>20</distance>
            <duration>5</duration>
            <repeat>3</repeat>
        </setup>
        <setup id="RadioactivityVsAbsorber">
            <name>Radioactivity versus Absorber Foils</name>
            <description>
                <p>Measure radioactivity through various absorbers at a fixed distance from the source.</p>
            </description>
            <absorberName>None,Alum.Foil x1,Alum.Foil x2,Alum.Foil x4,Alum.Foil x8,Alum.Foil x16,Alum.Foil x32,Alum.Foil x64,Alum.Foil x???</absorberName>
            <distance>20</distance>
            <duration>5</duration>
            <repeat>3</repeat>
        </setup>
        <setup id="SimActivityVsTime">
            <name>Radioactivity over Time (Simulated)</name>
            <description>
                <p>Measure radioactivity over time at a fixed distance from a simulated source.</p>
            </description>
            <distance>20</distance>
            <duration>5</duration>
