<?xml version="1.0" encoding="utf-8" ?>
<equipmentConfig title="Template" version="4.0">
    <powerupDelay>3</powerupDelay>
    <powerdownTimeout>15</powerdownTimeout>
    <devices>
        <device name="DeviceEquipment">
            <initialiseDelay>3</initialiseDelay>
        </device>
    </devices>

    <drivers>
        <driver name="DriverEquipment">
            <executionTimes>
                <initialise>2</initialise>
                <start>4</start>
                <run>6</run>
                <stop>3</stop>
                <finalise>1</finalise>
            </executionTimes>
            <experimentResults>
                <someParameter />
                <someResult />
            </experimentResults>
        </driver>
    </drivers>

    <validation>
        <someParameter>
            <minimum>10</minimum>
            <maximum>900</maximum>
        </someParameter>
    </validation>

    <setups>
        <setup id="Equipment">
            <driver>DriverEquipment</driver>
        </setup>
    </setups>
</equipmentConfig>
