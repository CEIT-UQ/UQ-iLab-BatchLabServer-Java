<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>SyncMachine LabServer</comment>

    <!-- DBHost: Database driver -->
    <entry key="DBDriver">org.postgresql.Driver</entry>
    <!-- DBUrl: Database url which includes the database name -->
    <entry key="DBUrl">jdbc:postgresql://localhost:5432/SyncMachine_LabServer</entry>
    <!-- DBPoolSize: Database connection pool size -->
    <entry key="DBPoolSize">1</entry>
    <!-- DBUser: Database user name, privileges need to be granted for this user to use the database -->
    <entry key="DBUser">LabServer</entry>
    <!-- DBPassword: Database user's password -->
    <entry key="DBPassword">ilab</entry>

    <!-- LogAuthentication: If true, log the ServiceBroker's guid and passkey -->
    <entry key="LogAuthentication">false</entry>

</properties>
